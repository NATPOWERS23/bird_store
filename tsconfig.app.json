/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@shared/*": ["./src/app/core/shared/*"],
      "@core/*": ["./src/app/core/*"],
      "@assets/*": ["./src/assets/*"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
